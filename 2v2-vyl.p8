function _move(pl)
	
end

function shoot(i,j)
 shot.x = i + 8
 shot.y = j
 shot.is_on = true
end
function _update()
	shot.x += 4
	if btn(0) then
		move(p1)
	end
	if btnp(4) then
		shoot(p1.x,p1.y)
	end
end

function _draw()
	cls()
	spr(5,p1.x,p1.y)
	if shot.is_on then
	 spr(shot.sp,shot.x,shot.y)
	end
end

